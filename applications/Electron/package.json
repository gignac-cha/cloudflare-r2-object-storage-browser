{
  "name": "@cloudflare-r2-object-storage-browser/electron",
  "version": "1.0.0",
  "description": "Cloudflare R2 Object Storage Browser - Electron Desktop Application",
  "main": "outputs/electron/main.js",
  "author": "Your Name",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "vite build && electron-builder",
    "build:mac": "npm run build -- --mac",
    "build:win": "npm run build -- --win",
    "build:linux": "npm run build -- --linux",
    "clean": "rm -rf outputs node_modules/.vite",
    "rebuild": "npm run clean && npm run build",
    "start": "NODE_ENV=production electron .",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"sources/**/*.{ts,tsx}\" \"electron/**/*.ts\""
  },
  "dependencies": {
    "@headlessui/react": "^2.1.10",
    "@monaco-editor/react": "^4.6.0",
    "@tanstack/react-table": "^8.20.5",
    "allotment": "^1.20.2",
    "axios": "^1.7.7",
    "bytes": "^3.1.2",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.446.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-pdf": "^9.1.1",
    "react-player": "^2.16.0",
    "uuid": "^10.0.0",
    "zustand": "^4.5.5"
  },
  "devDependencies": {
    "@types/bytes": "^3.1.4",
    "@types/node": "^20.14.0",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.8.1",
    "@vitejs/plugin-react": "^4.3.2",
    "autoprefixer": "^10.4.20",
    "electron": "^33.4.11",
    "electron-builder": "^25.1.8",
    "electron-store": "^8.2.0",
    "eslint": "^9.12.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.12",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.13",
    "tsup": "^8.5.0",
    "typescript": "^5.6.2",
    "vite": "^5.4.8",
    "vite-plugin-electron": "^0.28.8",
    "vite-plugin-electron-renderer": "^0.14.6"
  }
}
